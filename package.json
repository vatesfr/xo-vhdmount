{
  "name": "@nraynaud/xo-vhdmount",
  "version": "0.0.1",
  "license": "AGPL-3.0",
  "description": "Expose VHDs as block devices via Fuse",
  "keywords": [
    "vhd",
    "FUSE"
  ],
  "homepage": "https://github.com/vatesfr/xo-vhdmount/",
  "bugs": "https://github.com/vatesfr/xo-vhdmount/issues",
  "repository": {
    "type": "git",
    "url": "git://github.com/vatesfr/xo-vhdmount.git"
  },
  "preferGlobal": false,
  "main": "dist/",
  "bin": {},
  "files": [
    "dist/"
  ],
  "engines": {
    "node": ">=4"
  },
  "dependencies": {
    "@nraynaud/struct-fu": "^1.0.1",
    "@nraynaud/xo-fs": "^0.0.4",
    "babel-runtime": "^6.18.0",
    "exec-promise": "^0.6.1",
    "fs-promise": "^0.5.0",
    "fuse-bindings": "^2.9.0",
    "lodash": "^4.16.6",
    "promise-toolbox": "^0.7.0"
  },
  "devDependencies": {
    "babel-cli": "^6.18.0",
    "babel-eslint": "^7.1.0",
    "babel-plugin-transform-runtime": "^6.15.0",
    "babel-preset-env": "^0.0.7",
    "babel-preset-stage-0": "^6.16.0",
    "cross-env": "^3.1.3",
    "dependency-check": "^2.6.0",
    "ghooks": "^1.3.2",
    "rimraf": "^2.5.4",
    "standard": "^8.5.0"
  },
  "scripts": {
    "build": "cross-env NODE_ENV=production babel --source-maps --out-dir=dist/ src/",
    "clean": "rimraf dist/",
    "depcheck": "dependency-check ./package.json",
    "dev": "cross-env NODE_ENV=development babel --watch --source-maps --out-dir=dist/ src/",
    "lint": "standard",
    "posttest": "npm run lint && npm run depcheck",
    "prebuild": "npm run clean",
    "predev": "npm run clean",
    "prepublish": "npm run build"
  },
  "babel": {
    "plugins": [
      "transform-runtime"
    ],
    "presets": [
      [
        "env",
        {
          "targets": {
            "node": 4
          }
        }
      ],
      "stage-0"
    ]
  },
  "standard": {
    "ignore": [
      "dist"
    ],
    "parser": "babel-eslint"
  },
  "config": {
    "ghooks": {
      "commit-msg": "npm test"
    }
  }
}
